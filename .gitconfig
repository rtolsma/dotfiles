[user]
	name = Ryan Tolsma
	email = 1tolsmar@gmail.com

[alias]
	co = checkout
	s = status
	ri = rebase -i
	rc = rebase --continue
	pf = push --force-with-lease
	b = branch
	l = log

[fetch]
	prune = true
[submodule]
	recurse = true

[http]
	postBuffer = 1572864000
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
