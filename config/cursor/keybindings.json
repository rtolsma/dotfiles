// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+i",
        "command": "composerMode.agent"
    },
    {
        "key": "shift+enter",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "\\\r\n"
        },
        "when": "terminalFocus"
    },
    {
        "key": "alt+cmd+space",
        "command": "composer.toggleVoiceDictation",
        "when": "!terminalFocus"
    },
    {
        "key": "shift+cmd+space",
        "command": "-composer.toggleVoiceDictation",
        "when": "!terminalFocus"
    }
]